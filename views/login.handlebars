<form class="login-form col s12">
  <div class="row">
    <div class="col s3"></div>
    <div class="col s5">
      <h4>Login to place an order</h4>
    </div>
  </div>

  <div class="row">
    <div class="col s3"></div>
    <div class="input-field col s5">
      <input id="username" type="text" class="validate">
      <label for="username">Username</label>
    </div>
  </div>

  <div class="row">
    <div class="col s3"></div>
    <div class="input-field col s5">
      <input id="password" type="password" class="validate">
      <label for="password">Password</label>
    </div>
  </div>

  <div class="row">
    <div class="col s3"></div>
    <div class="col s3">
      <button id="submit" 
        class="btn-small waves-effect waves-dark blue accent-2" 
        type="submit" 
        name="action">Submit
      </button>
    </div>
    <div class="col s2">
      <a href="./createAccount">Create account</a>
    </div>
  </div>

  <div class="row">
    <div class="col s3"></div>
    <div class="col s9">
      <p>You're one click away from a delicious meal!</p>
    </div>
  </div>

</form>

<script src="./js/login.js"></script>

